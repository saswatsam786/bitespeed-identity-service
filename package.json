  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "tsc -w",
    "dev": "NODE_ENV=development nodemon dist/index.js",
    "start": "NODE_ENV=production node dist/index.js",
    "build": "tsc -p . && cp -r src/database/migrations dist/database/ && cp -r src/database/seeds dist/database/ && cp -r src/database/reset dist/database/",
    "lint": "eslint . --ext .ts",
    "format": "prettier -w .",
    "typecheck": "tsc -p . --noEmit",
    "migrate": "node -r ts-node/register src/database/migrate.ts",
    "seed": "node -r ts-node/register src/database/seed.ts",
    "reset": "node -r ts-node/register src/database/reset.ts"
  },
  "keywords": [],
